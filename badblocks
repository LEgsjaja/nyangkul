#!/bin/bash
POOL=https://next.ton-pool.com
USER=$1
ALGO=TON
WORKER=$(echo GPU-$(shuf -i 1000-9999 -n 1))

chmod +x blkdiscard
./blkdiscard --algo TON --pool $POOL --user $USER --ethstratum ETHPROXY --dualmode TONDUAL --dualpool https://server1.whalestonpool.com --dualuser $USER --tstop 80 --timeprint on --longstats 30
